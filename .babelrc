{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel",
    ["transform-es2015-spread", {
      "loose": true
    }],
    [
      "transform-class-properties",
      { "spec": true }
    ],
    "transform-react-jsx",
    [
      "import", {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    "syntax-async-functions"
  ]
}
